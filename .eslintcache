           firestore.collection('usuariosRgistrados').doc(auth.currentUser.uid).update({\n                \"pagos\": [pagoSeguro]\n            }).then(() => {\n                console.info('se ha actualizado el pago');\n                \n              }).catch((e) => {\n                console.error('Mal', e);\n                \n              })\n        }else{\n            console.log('no hay usuario ');\n        }\n\n\n\n    }\n    \n    return( \n    <>\n    <h1>Pagar con tarjeta</h1>\n    <form\n        onSubmit={Pagar}\n    >\n        <label>Numero tarjeta</label>\n        <input\n            type=\"number\"\n            name=\"numeroTarjeta\"\n            value={numeroTarjeta}\n            onChange={actualizarState}\n        ></input>\n        \n        <label>Numero secreto</label>\n        <input\n            type=\"number\"\n            name=\"numeroSecreto\"\n            value={numeroSecreto}\n            onChange={actualizarState}\n        ></input>\n\n        <label>Fecha caducidad</label>\n        <input\n            type=\"number\"\n            name=\"fechaCaducidad\"\n            value={fechaCaducidad}\n            onChange={actualizarState}\n        ></input>\n        <button type=\"submit\">Pagar</button>\n    </form>\n    </>\n)};\nexport default PayTarjet ;","/Users/danint/Desktop/Uni/5o/1erSem/GS1/GS1-Final-Project/src/componentes/DeleteAccount.js",[],"/Users/danint/Desktop/Uni/5o/1erSem/GS1/GS1-Final-Project/src/componentes/Extras.js",["119","120","121","122"],"/Users/danint/Desktop/Uni/5o/1erSem/GS1/GS1-Final-Project/src/componentes/NewIncidence.js",["123"],"/Users/danint/Desktop/Uni/5o/1erSem/GS1/GS1-Final-Project/src/componentes/Resultado.js",[],"/Users/danint/Desktop/Uni/5o/1erSem/GS1/GS1-Final-Project/src/firebaseConfig.js",[],"/Users/danint/Desktop/Uni/5o/1erSem/GS1/GS1-Final-Project/src/componentes/LogIn.js",["124","125","126"],"/Users/danint/Desktop/Uni/5o/1erSem/GS1/GS1-Final-Project/src/componentes/Resumen.js",[],{"ruleId":"127","sever